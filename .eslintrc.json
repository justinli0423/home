{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}],
        "max-len": [1, 150],
        "class-methods-use-this": 0,
        "no-restricted-syntax": 0,
        "react/jsx-no-bind": 0,
        "no-mixed-operators": 0
    },
    "env": {
        "browser": true
    }
}